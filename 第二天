#include <stdio.h>
int Max(int x,int y)
{
	if(x>y)
		return x;
	else

		return y;

}
int main()
{   
	int num1=10;
	int num2=20;
	int max=0;
		max=Max(num1,num2);
			printf("较大值=%d\n",max);
	return 0;
}

相同类型的
#include <stdio.h>
int main()
{
   int num1=20;
   int num2=30;
   if(num1>num2)
	   printf("num1>num2\n");
   else
	   printf("num1<num2\n");
	return 0;
}
